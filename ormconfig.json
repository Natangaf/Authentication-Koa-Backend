{
    "type": "postgres",
    "host": "postgres",
    "port": 5432,
    "username": "${DB_USER}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME}",
    "synchronize": true,
    "logging": false,
    "entities": ["src/entities/**/*.ts"],
    "migrations": ["src/migration/**/*.ts"],
    "subscribers": ["src/subscriber/**/*.ts"]
  }